{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": ["Google"],
      "minLength": 1,
      "maxLength": 55
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "format": "email",
      "examples": ["google@gmail.com"],
      "minLength": 1,
      "maxLength": 55
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "examples": ["google"],
      "minLength": 1,
      "maxLength": 55
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["password"],
      "minLength": 8,
      "maxLength": 55
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "format": "uri",
      "examples": ["https://logo.png"]
    }
  }
}
